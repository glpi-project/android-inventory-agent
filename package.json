{
  "name": "android-inventory-agent",
  "version": "1.0.0",
  "description": "Branch created with the intention of functioning as an introduction web site of the project, a small point of entry where anyone can find out what it is about and to be able to get relevant information.",
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "start": "bundle exec jekyll serve",
    "build": "bundle exec jekyll build",
    "install": "bundle install",
    "update-version": "ci/update_version_list.sh",
    "test-sass": "./node_modules/.bin/sass-lint -c app/config/.sass-lint.yml 'css/**/*.scss, _sass/**/*.scss' -v -q",
    "test-js": "./node_modules/.bin/eslint **/*.js  --fix",
    "test-yaml": "ci/yamllint.sh",
    "test": "npm run test-sass & npm run test-js & npm run test-yaml & npm run update-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyve-mdm/android-inventory-agent.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flyve-mdm/android-inventory-agent/issues"
  },
  "homepage": "http://flyve.org/android-inventory-agent/",
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-config-google": "^0.9.1",
    "htmlhint": "^0.10.1",
    "sass-lint": "^1.12.1",
    "yaml-lint": "^1.0.0"
  }
}
